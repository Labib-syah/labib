public class Praktikum1 {
    public static void main(String[] args) {
        //int bilangan [5] = {1,2,3,4,5}; akan error karena di dalam kurung siku seharusnya kosong
        int bilangan [] = {1,2,3,4,5};
        for (int i = 0; i <  bilangan.length; i++){
            System.out.print(bilangan[i]);
            //System.out.println(bilangan[6]); akan error karena hanya terdapat 4 indeks
            System.out.print(" ");
        }
        
    }
    
}
